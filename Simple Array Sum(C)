#include <assert.h>
#include <ctype.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(){
    int n, sum = 0;
    //printf("Enter the length of array");
    scanf("%d", &n);
    int arr[n];
    //printf("Enter the values of array one by one");
    for(int arr_i = 0; arr_i < n; arr_i++){
       scanf("%d",&arr[arr_i]);
    }
    for(int arr_i = 0; arr_i < n; arr_i++){
         sum = sum + arr[arr_i];
    }

    printf("%d", sum);

}





#include<stdio.h>
int main()
{
    int a[5],sum=0,i;
    printf("Enter the elements: ");
    for(i=0;i<5;i++)
    {
        scanf("%d",&a[i]);
    }
   
    for(i=0;i<5;i++)
    {
        sum=sum+a[i];
        
    }
    printf("%d",sum);
}
